@page
@using RestaurantOrderSystem.Models

@model RestaurantOrderSystem.Pages.OrderModel
@{
    <label>Order Id: @Model.Order.Id</label>
    List<Item> items = Model.Items.ToList();
    <label>Choose Items to add to your order</label>
    @foreach (Item item in items)
    {
        <table class="row">
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
        </table>
    }



}
